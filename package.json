{
  "name": "citi-web-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/citi-web-components/citi-web-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/allynkalda/Accordion-Component-Using-StencilJS.git"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepare": "husky install",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,html,json.js,css,scss}'",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "lint-staged": "lint-staged",
    "commit": "git cz"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {
    "@stencil/core": "^2.7.0"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "commitizen": "^4.2.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.26.1",
    "git-cz": "^4.8.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "puppeteer": "^10.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,css,json.js,css,scss}": [
      "prettier --write 'src/**/*.{ts,tsx,css,html,json.js,css,scss}'",
      "eslint src/**/*{.ts,.tsx} --fix"
    ]
  },
  "license": "MIT"
}
